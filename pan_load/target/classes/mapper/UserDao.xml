<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mj.pan_load.dao.UserDao">
    <select id="su" resultType="User">
        select * from d_user where d_user.name = #{name} and d_user.password = #{password}
    </select>
</mapper>